		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.1.7",
		"bits-ui": "^2.2.0",
		"clsx": "^2.1.1",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.2.0",
		"mode-watcher": "^1.0.7",
