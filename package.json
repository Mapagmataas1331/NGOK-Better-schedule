		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.7",
		"tw-animate-css": "^1.3.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.33.0",
		"vite": "^6.3.5"
