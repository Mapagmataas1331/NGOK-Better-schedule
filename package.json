{
	"name": "ma-ngok",
	"version": "1.0.0",
	"private": true,
	"description": "NGOK Better schedule",
	"homepage": "https://ngok.ma.cyou",
	"repository": "github:mapagmataas1331/NGOK-Better-schedule",
	"author": "Mapagmataas",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"postbuild": "npx svelte-sitemap --domain https://ngok.ma.cyou --out-dir build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "gh-pages -d build",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"preview:gh": "http-server build -p 80",
		"validate": "npm run check && prettier --write . && eslint ."
	},
	"devDependencies": {
		"@internationalized/date": "^3.7.0",
		"@lucide/svelte": "^0.479.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.19.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.13.10",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.3.11",
		"clsx": "^2.1.1",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.1.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.0.0",
		"lucide-svelte": "^0.479.0",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass-embedded": "^1.85.1",
		"svelte": "^5.23.0",
		"svelte-check": "^4.1.5",
		"svelte-sitemap": "^2.7.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1",
		"vite": "^6.2.1"
	}
}
