{
	"name": "ma-ngok",
	"version": "1.0.0",
	"private": true,
	"description": "NGOK Better schedule",
	"homepage": "https://ngok.ma.cyou",
	"repository": "github:mapagmataas1331/NGOK-Better-schedule",
	"author": "Mapagmataas",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"postbuild": "npx svelte-sitemap --domain https://ngok.ma.cyou --out-dir build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "gh-pages -d build -t true",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"preview": "vite preview",
		"preview:gh": "http-server build -p 80",
		"validate": "npm run check && prettier --write . && eslint ."
	},
	"devDependencies": {
		"@internationalized/date": "^3.7.0",
		"@sveltejs/adapter-static": "^3.0.6",
		"@sveltejs/kit": "^2.12.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.2",
		"@types/eslint": "^9.6.1",
		"@types/node": "^22.10.2",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.78",
		"clsx": "^2.1.1",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"gh-pages": "^6.2.0",
		"globals": "^15.13.0",
		"lucide-svelte": "^0.468.0",
		"mode-watcher": "^0.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"sass-embedded": "^1.83.0",
		"svelte": "^5.14.0",
		"svelte-check": "^4.1.1",
		"svelte-sitemap": "^2.6.0",
		"svelte-sonner": "^0.3.28",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.3.1",
		"tailwindcss": "^3.4.16",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.1",
		"vite": "^6.0.3"
	}
}
